
<%= form_with(model: @note, url: upload_documents_note_path(@note), method: :patch, local: true, html: { multipart: true }) do |form| %>
  <div>
    <%= form.label :documents, "Add documents" %><br>
    <%= form.file_field :documents, multiple: true, class: "form-control", type: "file", id:"formFile" %>
  </div>
<br>
  <div>
    <%= form.submit "Upload", class: "btn btn-outline-info btn-lg",style: "color: white;"%>
  </div>
<% end %>

