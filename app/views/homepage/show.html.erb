<h1><%= @homepage.title%></h1>
<h3>Your current subjects</h3>
<table class="table table-bordered">
    <thead>
        <tr class="table-dark">
            <th scope = "col">Subject Name</th>
            <th scope = "col">View</th>
            <th scope = "col">Edit</th>
            <th scope = "col">Delete</th>
            </tr>
        </thead>
<%@homepage.notes.each do |note|%>
        <tbody>
            <tr class="table-dark">
            <th scope = "col"><%=note.title%></th>
            <th scope = "col"><%=link_to "See Notes", note, class: "text-info"%><br></th>
            <th scope = "col"><%=link_to "Edit Subject", edit_note_path(note), class: "text-info"%><br></th>
            <th scope = "col"><%=button_to "Delete Subject ", note, method: :delete,class: "btn btn-outline-info",style: "color: white;"%><br></th>
            </tr>
        </tbody>
<%end%>
</table>
<%= link_to "Make a Subject", new_note_path,class: "btn btn-outline-info",style: "color: black;"%>
