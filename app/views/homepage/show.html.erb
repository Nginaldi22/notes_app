<h1><%= @homepage.title%></h1>
<h3>Your current subjects</h3>
<table class="table table-bordered">
    <thead>
        <tr>
            <th scope = "col">Subject Name</th>
            <th scope = "col">View</th>
            <th scope = "col">Edit</th>
            <th scope = "col">delete</th>
            </tr>
        </thead>
<%@homepage.notes.each do |note|%>
        <tbody>
            <tr>
            <th scope = "col"><%=note.title%></th>
            <th scope = "col"><%=link_to "view", note%><br></th>
            <th scope = "col"><%=link_to "edit", edit_note_path(note)%><br></th>
            <th scope = "col"><%=button_to "delete", note, method: :delete%><br></th>
            </tr>
        </tbody>
<%end%>
</table>
<%= link_to "Make a Subject", new_note_path%>
